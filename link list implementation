#include <iostream>
using namespace std;
struct Node {
    int data;
    Node*next;
    
};
Node *head =NULL;
void insertEnd(int num){
    Node*newNode=new Node();
    newNode->data=num;
    newNode->next=NULL;
    if(head==NULL){
        head=newNode;
        cout<<num<<"insert"<<endl;
    }
    else{
        Node*temp=head;
        while(temp->next !=NULL){
            temp=temp->next;
            
        }
        temp->next=newNode;
        cout<<num<<"inserted"<<endl;
    }
}
int   main(){
    insertEnd(10);
    insertEnd(20);
    insertEnd(30);
    insertEnd(40);
}                
