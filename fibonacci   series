#include <iostream>
using namespace std;// Required for std::gcd

int fib (int num){
    if(num==0){
        return 0;
    }
    if (num==1){
        return 1;
    }
    else{
        return fib(num-1) + fib(num-2);
    }
}
int main (){
    int n;
    cout << "enter a number:";
    cin>>n;
    for(int i=0;i<n;i++){
        cout<<fib(i)<<" ";
        
    }
}  
